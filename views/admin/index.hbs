<input type="hidden" name="" id="dataForChart" value="{{bookingsByMonthJSON}}">

<div >
    <h1>Monthly booking summary</h1>
  <canvas   id="myChart"></canvas>
</div>

<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    const ctx = document.getElementById('myChart');

    
    const dataForChartInput = document.getElementById('dataForChart');
    const bookingsByMonthJSON = dataForChartInput.value;
       console.log("kfksffk",bookingsByMonthJSON)
    
    const bookingsByMonth = JSON.parse(bookingsByMonthJSON);

   
    const monthNames = Object.keys(bookingsByMonth)
    const data = Object.values(bookingsByMonth);

    new Chart(ctx, {
      type: 'bar',
      data: {
        labels: monthNames,
        datasets: [{
          label: '# of Bookings',
          data: data,
          backgroundColor: 'rgba(54, 162, 235, 0.2)',
          borderColor: 'rgba(54, 162, 235, 1)',
          borderWidth: 1
        }]
      },
      options: {
        scales: {
          y: {
            beginAtZero: true
          }
        }
      }
    });
  });

    
    
     
</script>

<div class="charts-container">
    <div class="chart-wrapper">
        <input type="hidden" name="" id="doughnutChartData" value='{{roomDatajson}}'>
        <div style="height: 300px;">
            <h1>Booking Summary Based on Rooms</h1>
            <canvas id="doughnutChart"></canvas>
        </div>
    </div>

    <div class="chart-wrapper">
        <input type="hidden" name="" id="pieChartData" value='{{bookingsByVendorJSON}}'>
        <div style="height: 300px;">
            <h1>Vendors Booking</h1>
            <canvas id="pieChart"></canvas>
        </div>
    </div>
</div>

<script>
document.addEventListener('DOMContentLoaded', function () {
    // Doughnut Chart
    const doughnutCtx = document.getElementById('doughnutChart');
    const doughnutInput = document.getElementById('doughnutChartData');
    const doughnutDataJSON = doughnutInput.value;
    console.log(doughnutDataJSON);
    const doughnutData = JSON.parse(doughnutDataJSON);

    const roomNames = Object.keys(doughnutData);
    const roomCounts = Object.values(doughnutData);

    new Chart(doughnutCtx, {
        type: 'doughnut',
        data: {
            labels: roomNames,
            datasets: [{
                label: '# of Bookings',
                data: roomCounts,
                backgroundColor: [
                    'rgba(255, 99, 132, 0.5)',
                    'rgba(54, 162, 235, 0.5)',
                    'rgba(255, 205, 86, 0.5)',
                    'rgba(75, 192, 192, 0.5)',
                    'rgba(153, 102, 255, 0.5)',
                    'rgba(255, 159, 64, 0.5)'
                ],
                borderColor: [
                    'rgba(255, 99, 132, 1)',
                    'rgba(54, 162, 235, 1)',
                    'rgba(255, 205, 86, 1)',
                    'rgba(75, 192, 192, 1)',
                    'rgba(153, 102, 255, 1)',
                    'rgba(255, 159, 64, 1)'
                ],
                borderWidth: 1
            }]
        },
        options: {
            responsive: true,
            plugins: {
                legend: {
                    position: 'top',
                },
                title: {
                    display: true,
                    text: 'Room Bookings'
                }
            }
        }
    });

    // Pie Chart
    const pieCtx = document.getElementById('pieChart');
    const pieInput = document.getElementById('pieChartData');
    const pieDataJSON = pieInput.value;
    console.log(pieDataJSON);
    const pieData = JSON.parse(pieDataJSON);

    const vendorNames = Object.keys(pieData);
    const vendorCounts = Object.values(pieData);

    new Chart(pieCtx, {
        type: 'pie',
        data: {
            labels: vendorNames,
            datasets: [{
                label: '# of Bookings',
                data: vendorCounts,
                backgroundColor: [
                    'rgba(255, 99, 132, 0.5)',
                    'rgba(54, 162, 235, 0.5)',
                    'rgba(255, 205, 86, 0.5)',
                    'rgba(75, 192, 192, 0.5)',
                    'rgba(153, 102, 255, 0.5)',
                    'rgba(255, 159, 64, 0.5)'
                ],
                borderColor: [
                    'rgba(255, 99, 132, 1)',
                    'rgba(54, 162, 235, 1)',
                    'rgba(255, 205, 86, 1)',
                    'rgba(75, 192, 192, 1)',
                    'rgba(153, 102, 255, 1)',
                    'rgba(255, 159, 64, 1)'
                ],
                borderWidth: 1
            }]
        },
        options: {
            responsive: true,
            plugins: {
                legend: {
                    position: 'top',
                },
                title: {
                    display: true,
                    text: 'Vendors Booking'
                }
            }
        }
    });
});
</script>

                        

