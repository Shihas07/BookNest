<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script>
    function openEditModal(categoryId, categoryName) {
        document.getElementById('editRoomId').value = categoryId;
        document.getElementById('editCategory').value = categoryName;
        $('#editRoomModal').modal('show');

        document.getElementById("buttonEditCategory").addEventListener("click",()=>{
            let id = document.getElementById("editRoomId").value
            let category = document.getElementById("editCategory").value
            console.log("ddd:",id,category)
            const data = {
                id,
                category
            }

            console.log(data)

            axios.post("/admin/editCategory",{id:id, category:category})
            .then((data)=>{
                window.location.href="/admin/category"
            })
            .catch((error)=>{
                console.log(error)
            })

        })
    }
</script>

